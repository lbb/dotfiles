urxvt*scrollBar:	false
urxvt*secondaryScroll:	true
urxvt*saveLines:	65535


URxvt*shading:20
urxvt*background: rgba:0000/0000/0200/c800
URxvt*foreground: #eeeeee

URxvt.font: xft:Source\ Code\ Pro\ Semibold:pixelsize=13:antialias=true:hinting=true,FontAwesome:size=13 

! Imports:
URxvt.perl-ext-common: default,matcher,resize-font,selection-to-clipboard

! Scripting
! Urls:
 
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1

URxvt.matcher.rend.0: Uline Bold fg5

! Resize

URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up



xterm*termName: xterm-256color
XTerm*locale: true
XTerm*metaSendsEscape: true
Xterm*saveLines: 4096
xterm*bellIsUrgent: true
XTerm*faceName: SauceCodePro Nerd Font
XTerm*faceSize: 10
! special
*.foreground:   #dedede
*.background:   #191919
*.cursorColor:  #dedede

! black
*.color0:       #2e3436
*.color8:       #555753

! red
*.color1:       #cc0000
*.color9:       #ef2929

! green

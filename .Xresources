URxvt.scrollBar: false
URxvt.font: xft:Inconsolata for Powerline:pixelsize=13:antialias=true:hinting=true
URxvt*dynamicColors: on
URxvt.cursorBlink: 1
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

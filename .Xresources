! General
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.clipboard.autocopy: If true, PRIMARY overwrites clipboard
URxvt.perl-ext-common: default,matcher,clipboard
urxvt*termName:  rxvt-256color
urxvt*loginShell:         true
urxvt*scrollBar:          false
!urxvt*scrollBar_right:    true
!urxvt*scrollstyle:        plain
urxvt*secondaryScroll:    true
urxvt*saveLines:         65535
urxvt*cursorBlink:       false
urxvt*urgentOnBell:       true
!urxvt*override-redirect:false
!urxvt*borderLess:        true
urxvt*internalBorder:       0
urxvt*externalBorder:       0
!clickable URLS
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5
URxvt.clipboard.copycmd:  xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard

! Appearance
! urxvt*font: xft:Source\ Code\ Pro:Medium:pixelsize=16:antialias=true:hinting=true,Source\ Code\ Pro\ for\ Powerline\:Medium:pixelsize=16:antialias=true:hinting=true,xft:Source\ Code\ Powerline\ for\ Powerline:Medium:pixelsize=13:antialias=true:hinting=true
! urxvt*font: xft:Deja Vu Sans Mono for Powerline:Medium:pixelsize=10:antialias=true:hinting=true,xft:Powerline\ Symbols:Medium:pixelsize=10,antialias=true:hinting=true,xft:PowerlineSymbols:Medium:pixelsize=10:antialias=true:hinting=true
